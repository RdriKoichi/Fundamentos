#include <stdio.h>
#include <stdlib.h>

int main() {

    int i, soma = 0;                   //soma de vetor
    int vetor[5] = {8,15,16,1,4};

    for(i=0; i<5; i++) {
        soma = soma + vetor[i];
        printf("vetor[%i] = %i\n",i, vetor[i]);
        printf("Soma = %d\n", soma);
    }
    
    return 0;
}
