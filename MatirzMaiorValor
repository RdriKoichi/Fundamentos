#include <stdio.h>

int main(){

    int matriz[5][5], i, j;
    int maior, linha, coluna;

    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            printf("matriz[%d][%d]: ",i,j);
            scanf("%d",&matriz[i][j]);
        }
    }
    for (int i = 0; i < 5; i++) {
        printf("\n");
        for (int j = 0; j < 5; j++) {
            printf("%d ", matriz[i][j]);
        }
    }

    maior = matriz[0][0];

    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 5; j++) {
            if (matriz[i][j] > maior) {
                maior = matriz[i][j];
                linha = i;
                coluna = j;
            }
        }
    }

    printf("\n\nMaior matriz[%d][%d]: %i \n", linha, coluna, maior);

    return 0;
}
