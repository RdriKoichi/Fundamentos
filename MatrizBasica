#include <stdio.h>

int main(){
    /*
    int matriz[5][5], i, j; // coloca os valores q vc quiser

    for (int i = 0; i < 5 ; i++) {
        for (int j = 0; j < 5; j++) {
            printf("matriz[%d][%d]: ",i,j);
            scanf("%d",&matriz[i][j]);
        }
