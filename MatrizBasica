#include <stdio.h>

int main(){
    int matriz[5][5], i, j;

    for (int i = 0; i < 5 ; i++) {
        for (int j = 0; j < 5; j++) {
            printf("matriz[%d][%d]: ",i,j);
            scanf("%d",&matriz[i][j]);
        }
    }
    for (int i = 0; i < 5 ; i++) {
        printf("\n");
        for (int j = 0; j < 5; j++) {
            printf("%d ",matriz[i][j]);
        }
    }
    return 0;
}
