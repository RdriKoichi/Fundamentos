     */
    int matriz[5][5], i, j;  //valores prontos 
    for(i=0;i<5;i++){
        for(j=0;j<5;j++){
            if(i == j)
                    matriz[i][j] = 1;
            else
                    matriz[i][j] = 0;
            }
        }
    for (int i = 0; i < 5 ; i++) {
        printf("\n");
        for (int j = 0; j < 5; j++) {
            printf("%d ",matriz[i][j]);
        }
    }
    
    return 0;
}
